{
  "name": "playwright",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "postinstall": "npx playwright install firefox --with-deps",
    "test": "npx playwright test",
    "test:headed": "npx playwright test --headed",
    "test:debug": "npx playwright test --debug",
    "test:ui": "npx playwright test --ui",
    "test:slowmo": "npx playwright test --headed --slow-mo=500",
    "test:report": "npx playwright test --reporter=html && npx playwright show-report",
    "test:filter": "npx playwright test -g",
    "codegen": "npx playwright codegen",
    "clean": "rm -rf playwright-report test-results",
    "lint": "eslint --fix",
    "format": "prettier --write \"**/*.{js,ts,json,md}\""
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "devDependencies": {
    "@playwright/test": "^1.50.1",
    "@types/node": "^22.13.1",
    "@typescript-eslint/eslint-plugin": "^8.23.0",
    "@typescript-eslint/parser": "^8.23.0",
    "eslint": "^9.20.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jsdoc": "^50.6.3",
    "eslint-plugin-prettier": "^5.2.3",
    "eslint-plugin-promise": "^7.2.1",
    "prettier": "^3.4.2"
  }
}
